{
  "name": "yjx/easy-di",
  "description": "An easy dependencies inject container, which implements Psr-11.",
  "type": "library",
  "keywords": ["di", "dependency injection", "container", "ioc", "psr-11", "psr11", "container-interop"],
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "EasyDI\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "EasyDI\\Test\\": "tests/"
    }
  },
  "require": {
    "psr/container": "^1.0",
    "php": "^5.4 || ^7.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^7.1"
  },
  "provide": {
    "psr/container-implementation": "^1.0"
  }
}
